{
  "name": "tsnode-test-api",
  "version": "6.2.1",
  "description": "A node express typescript Rest API Boilerplate",
  "author": "John Livee G. Oroncillo",
  "scripts": {
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "dev": "nodemon ./core/server.ts",
    "dev:event": "ts-node -r dotenv/config -e \"import { Events} from './core/libs/Events'; const event = new Events(); event.startServer();\" ",
    "start": "node build/migrate.js up && node build/core/server",
    "clean": "rimraf build",
    "build": "npm run clean && tsc && cpx \"**/*.yml\" build",
    "migrate": "ts-node migrate.ts up",
    "migrate:refresh": "ts-node migrate.ts down && npm run migrate",
    "req": "npm i -g ts-node",
    "lint": "eslint --ext .ts ./ --fix",
    "test": "jest --silent",
    "test:log": "jest",
    "cron": "ts-node -r dotenv/config -e \"import { loadCron } from './core/'; loadCron();\"",
    "seed": "ts-node -r dotenv/config -e \"import { UserSeeder } from './seeder/UserSeeder'; UserSeeder.seedUser();\"",
    "make:model": "ts-node artisan.ts make:model",
    "make:repository": "ts-node artisan.ts make:repository",
    "make:migration": "ts-node artisan.ts make:migration",
    "make:api": "ts-node artisan.ts make:api",
    "make:service": "ts-node artisan.ts make:service",
    "make:event": "ts-node artisan.ts make:event",
    "make:cron": "ts-node artisan.ts make:cron",
    "make:doc": "ts-node artisan.ts make:doc",
    "make:socket": "ts-node artisan.ts make:socket",
    "apidoc": ".\\node_modules\\.bin\\apidoc -i docs -o docs",
    "generate:doc": "ts-node -r dotenv/config -e \"import { generate } from './core/libs/DocsBuilder'; generate();\"",
    "delete:doc": "ts-node -r dotenv/config -e \"import { deleteDocs } from './core/libs/DocsBuilder'; deleteDocs();\"",
    "build:doc": "npm run generate:doc && npm run apidoc && npm run delete:doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnliveeoroncillo/tsnode-rest-boilerplate.git"
  },
  "license": "ISC",
  "dependencies": {
    "@joi/date": "^2.1.0",
    "axios": "^0.23.0",
    "bcrypt": "^5.0.1",
    "bson": "^5.4.0",
    "case-anything": "^1.1.5",
    "cli-table": "^0.3.11",
    "colors": "1.3.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.4.0",
    "express-list-routes": "^1.1.3",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "httpolyglot": "^0.1.2",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "mongodb": "3.6.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql-migrations": "^1.0.7",
    "net": "^1.0.2",
    "node-cron": "^3.0.0",
    "node-libcurl": "^3.0.0",
    "nodemailer": "^6.7.8",
    "path": "^0.12.7",
    "pg": "^8.9.0",
    "pluralize": "^8.0.0",
    "predis": "^1.1.4",
    "prettier": "^2.2.1",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "ts-node": "^10.5.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5",
    "uuid": "^9.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.11.23",
    "@types/nodemailer": "^6.4.4",
    "@types/pluralize": "0.0.29",
    "@types/uuid": "^9.0.0",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "apidoc": "^0.51.0",
    "cpx2": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.4.5",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.0.0"
  }
}
