{
  "name": "tsnode-test-api",
  "version": "1.0.0",
  "description": "A node express typescript Rest API Boilerplate",
  "author": "John Livee G. Oroncillo",
  "scripts": {
    "dev": "nodemon ./core/server.ts",
    "start": "cross-env NODE_ENV=production node build/core/server",
    "clean": "rimraf build",
    "build": "npm run clean && tsc &&  cpx \"**/*.yml\" build",
    "migrate": "ts-node migrate.ts",
    "migrate:refresh": "ts-node migrate.ts down && npm run migrate",
    "req": "npm i -g ts-node",
    "lint": "eslint --ext .ts apis/ --fix",
    "test": "jest --silent",
    "test:log": "jest",
    "cron": "ts-node -r dotenv/config -e \"import { loadCron } from './core/'; loadCron();\"",
    "seed": "ts-node -r dotenv/config -e \"import { UserSeeder } from './seeder/UserSeeder'; UserSeeder.seedUser();\"",
    "make:model": "ts-node artisan.ts make:model",
    "make:repository": "ts-node artisan.ts make:repository",
    "make:migration": "ts-node artisan.ts make:migration",
    "make:api": "ts-node artisan.ts make:api",
    "make:service": "ts-node artisan.ts make:service",
    "make:cron": "ts-node artisan.ts make:cron"
  },
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/moment-timezone": "^0.5.30",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "@types/pluralize": "0.0.29",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "axios": "^0.23.0",
    "bcrypt": "^5.0.1",
    "case-anything": "^1.1.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.2",
    "express-list-routes": "^1.1.3",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.0",
    "node-libcurl": "^2.3.4",
    "nodemailer": "^6.7.2",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "prettier": "^2.2.1",
    "querystring": "^0.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.11.23",
    "cpx2": "^4.2.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.0.0"
  }
}
